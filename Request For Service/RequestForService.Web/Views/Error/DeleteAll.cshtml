@using RequestForService.Web.Extentions
@model RequestForService.Web.ViewModels.Base.ViewModelBase
@{ ViewBag.Title = "Error Logs Delete Confirmation!";}
<p>
	<h2>
		@ViewBag.Title
	</h2>
</p>
@using (Html.BeginForm())
{
	@Html.AntiForgeryToken()
	@*@Html.ValidationSummary()*@
	@*@Html.PartialFor(model => model.Result, "_ResultMessage")*@
	<div class="strip-noborder">
		<p>
			Please confirm that you want to and are allowed to delete all the error logs?
		</p>
	</div>
	<div class="strip-bordered">
		<input type="submit" value="Yes" class="btn btn-danger" />
		<a href="@Url.Action("List")" class="btn btn-default">No</a>
	</div>
}